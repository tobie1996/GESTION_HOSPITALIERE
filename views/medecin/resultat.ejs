       <div class="container">
        <!----  Start  reciproque ----->	
          <% resultat.forEach((resultat, index) => { %>
            <button style="height: 40px"  class="btn btn-outline-dark btn-block text-left mt-2" type="button" data-toggle="collapse" data-target="#collapsetwo<%= index %>" aria-expanded="false" aria-controls="collapsetwo<%= index %>">
            <%= resultat.nom %>
            <span class="fas fa-chevron-down float-right" aria-hidden="true"></span>
            </button>
            <div id="collapsetwo<%= index %>" class="collapse">
            <div class="card card-body">
            <ul>
            
              <li>
                <label class="text-info">Résultats analyse :</label>
                <p name="resultat_analyse"><%= resultat.resultat_analyse %></p>
              </li>

              <li>
                <label class="text-info">Résultats radiographie :</label>
                <p name="resultat_radio"><%= resultat.resultat_radio %></p>
              </li>
              <li>
                <label class="text-info">Portable :</label>
                <p name="tel"><%= resultat.tel %></p>
              </li>

            </ul>
              <a href="#modifossiertmodal<%= resultat.id %>" class="modifier" data-toggle="modal">
                <span style="display: flex; justify-content: flex-end;" >
                  <i class="fas fa-edit" style="color: rgb(49, 7, 126); font-size: 30px;"></i>
                </span>
              </a>
            </div>
            </div>
            <br>
            <% }) %>  


            <!----  modifier un dosssier ----->	 

            <% for (let i=0; i<resultat.length; i++) { %>
              <div class="modal fade" id="modifossiertmodal<%= resultat[i].id %>" tabindex="-1" role="dialog" aria-labelledby="mymodallabel" aria-hidden="true">
                <div class="modal-dialog modal-xl">
                  <div class="modal-content">
                    <div class="modal-header bg-dark">
                      <h5 class="modal-title text-white" id="mymodallabel">Résultat des examens de Mr/Mme <%= resultat[i].nom %></h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="close">
                        <span aria-hidden="true">×
                      </button>
                    </div>
                    <div class="modal-body">
                      <form method="post" action="/edit10/<%= resultat[i].id %>">
                        <div class="row">
                          <div class="col-md-6">
                            <label for="id" class="text-danger">numero test</label>
                            <input type="text" class="form-control" id="id" name="id" value="<%= resultat[i].id %>" readonly>
                          </div>
                          <div class="col-md-6">
                            <label for="email" class="text-success">Adresse email</label>
                            <input type="text" class="form-control" id="email" name="email" value="<%= resultat[i].email %>" readonly>
                          </div>
                          <div class="col-md-6">
                            <label for="tel" class="text-success">Portable</label>
                            <input type="text" class="form-control" id="tel" name="tel" value="<%= resultat[i].tel %>" readonly>
                          </div>
                          <div class="col-md-6">
                            <label for="email" class="text-success">Service</label>
                            <input type="text" class="form-control"  value="Service médical" readonly>
                          </div>
                          
                          <div class="col-md-6">
                            <label for="resultat_analyse" class="text-success">résultat examen d'analyse</label>
                            <textarea class="form-control" id="resultat_analyse" name="resultat_analyse" rows="8" cols="50"><%= resultat[i].resultat_analyse %></textarea>
                          </div>
                          <div class="col-md-6">
                            <label for="resultat_radio" class="text-success">résultat examen radiographie</label>
                            <textarea class="form-control" id="resultat_radio" name="resultat_radio" rows="8" cols="50"><%= resultat[i].resultat_radio %></textarea>                          </div>
                        </div>
                        <div class="modal-footer">
                          <button type="button" class="btn btn-danger" data-dismiss="modal">fermer</button>
                          <button type="submit" id="btnenregistrer" class="btn btn-primary">sauvegarder</button>
                        </div>
                      </form>
                    </div>
                  </div>
                </div>
              </div>
              <% } %>
                       
    <!----  fin creer un dosssier --------->

  
